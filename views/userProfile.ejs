<!doctype html>
<html lang="zxx">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Fashionoo</title>
    <link rel="icon" href="/img/favicon.png">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <!-- animate CSS -->
    <link rel="stylesheet" href="/css/animate.css">
    <!-- owl carousel CSS -->
    <link rel="stylesheet" href="/css/owl.carousel.min.css">
    <!-- font awesome CSS -->
    <link rel="stylesheet" href="/css/all.css">
    <!-- flaticon CSS -->
    <link rel="stylesheet" href="/css/flaticon.css">
    <link rel="stylesheet" href="/css/themify-icons.css">
    <!-- font awesome CSS -->
    <link rel="stylesheet" href="/css/magnific-popup.css">
    <!-- swiper CSS -->
    <link rel="stylesheet" href="/css/slick.css">
    <!-- style CSS -->
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/stylee.css">
    <link rel="stylesheet" href="/css/nice-select.css">

</head>

<body style="background-color: rgba(170, 71, 124, 0.068)">
   
    
  <!-- Header Section Begin -->

  <header
    style="background-color: rgb(255, 255, 255)"
    class="main_menu home_menu"
  >
    <div class="container">
      <div class="row align-items-center justify-content-center">
        <div class="col-lg-12">
          <nav class="navbar navbar-expand-lg navbar-light">
            <button
              class="navbar-toggler"
              type="button"
              data-toggle="collapse"
              data-target="#navbarSupportedContent"
              aria-controls="navbarSupportedContent"
              aria-expanded="false"
              aria-label="Toggle navigation"
            >
              <span><i></i></span>
            </button>
            <img
              style="width:300px; height:100px;"
              src="/img/logo.png"
              alt=""
            />
           
            <div
              class="collapse navbar-collapse main-menu-item"
              id="navbarSupportedContent"
            >
              <ul class="navbar-nav">
                <li class="nav-item">
                  <a class="nav-link" href="/">Home</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/products">Products</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/about">about</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" href="/contact">Contact</a>
                </li>
              </ul>
            </div>

            <div class="hearer_icon d-flex align-items-center">
              <a href="/logout">
                <ion-icon
                  style="
                    height: 25px;
                    width: 25px;
                    margin-left: 30px;
                    margin-top: 2px;
                    color: black;
                  "
                  name="log-out-outline"
                ></ion-icon>
              </a>
              <a href="/cart">
                <ion-icon
                  style="
                    height: 25px;
                    width: 25px;
                    margin-left: 30px;
                    margin-top: 2px;
                    color: black;
                  "
                  name="cart-outline"
                ></ion-icon>
              </a>
              <a href="/profile">
                <ion-icon
                  style="
                    height: 25px;
                    width: 25px;
                    margin-left: 30px;
                    margin-top: 2px;
                    color: black;
                  "
                  name="person-circle-outline"
                ></ion-icon>
              </a>

              <a href="/wishlist">
                <ion-icon
                  style="
                    height: 25px;
                    width: 25px;
                    margin-left: 30px;
                    margin-top: 2px;
                    color: black;
                  "
                  name="heart-outline"
                ></ion-icon>
              </a>
              <span class="ml-1"> </span>
            </div>
          </nav>
        </div>
      </div>
    </div>
   
  </header>
  <div style="margin-top: 5px; display: flex; justify-content: center">
    <h2
    style="font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; font-weight: bold; color: rgba(184, 103, 153, 0.767);">
    Profile</h2>
  </div>

  <div class="container rounded bg-white mt-0 mb-5">
    <div class="row">
      <div class="col-md-3 border-right">
        <div class="d-flex flex-column align-items-center text-center">
          <img
            class="rounded-circle mt-3"
            width="150px"
            src="https://t4.ftcdn.net/jpg/03/32/59/65/360_F_332596535_lAdLhf6KzbW6PWXBWeIFTovTii1drkbT.jpg"
          /><span class="font-weight-bold"><%=user.name %></span
          ><span class="text-black-50"><%=user.email %></span><span> </span>
        </div>
        <div class="mt-2 text-center">
          <a href="/edit-profile"
            ><button class="btn_1" type="button">
              Edit Profile
            </button></a
          >
        </div>
        <div class="mt-2 text-center">
          <a href="/orders"
            ><button class="btn_1" type="button">
              Orders
            </button></a
          >
        </div>
        <div class="mt-2 mb-2 text-center">
          <a href="/add-address"
            ><button class="btn_1" type="button">
              Add Address
            </button></a
          >
        </div>
      </div>
      <div class="col-md-9 ">
        <!-- <h4 style="margin:20px;">Address</h4> -->
        <div style="margin: 40px" class="row">
          <div class="col-md-4 mt-10">
            <% user.address.forEach((item,index)=>{ %>
            <div class="address-box">
             
              <div class="manage-address text-white">
                
                <div style="margin-top: 5px; display: flex; justify-content: center">
                    <h4
                    style="font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; font-weight: bold; color: rgba(184, 103, 153, 0.767);">
                     Address <%=index+1 %> </h4>
                  </div>
                <!-- <h5>address </h5> -->
                <b
                  style="
                    color: rgba(26, 24, 24, 0.836);
                    font-family: cursive;
                  "
                  >Name :  <%=item.name%></b
                ><br />
                <b
                  style="
                    color: rgba(26, 24, 24, 0.836);
                    font-family:  cursive;
                  "
                  >Mobile :  <%=item.mobile%> </b
                ><br />
                <b
                  style="
                    color:  rgba(26, 24, 24, 0.836);
                    font-family:  cursive;
                  "
                  >Village : <%=item.village%></b
                ><br />
                <b
                  style="
                    color: rgba(26, 24, 24, 0.836);
                    font-family:cursive;
                  "
                  >Land mark : <%=item.landmark%></b
                ><br />
                <b
                  style="
                    color: rgba(26, 24, 24, 0.836);
                    font-family:cursive;
                  "
                  >House name :<%=item.housename%> </b
                ><br />
                <b
                  style="
                    color:  rgba(26, 24, 24, 0.836);
                    font-family: cursive;
                  "
                  >City :  <%=item.city%></b
                ><br />
                <b
                  style="
                    color:  rgba(26, 24, 24, 0.836);
                    font-family: cursive;
                  "
                  >Pincode : <%=item.pincode%></b
                >
              </div>
              <div style="display: flex" class="mt-3 text-center">
                <a href="/delete-address/<%=item._id%>"
                  ><button
                    class="btn_1"
                    type="button"
                    onclick="return confirm('Are you sure you want to delete address')"
                  >
                    delete
                  </button></a
                >

                <a href="/edit-address/<%=item._id%>"
                  ><button
                    style="margin-left: 10px"
                    class="btn_1"
                    type="button"
                  >
                    Edit
                  </button></a
                >
              </div>
            </div>
            <% }) %>
          </div>
        
        </div>
      </div>
    </div>
  </div>

  <!-- <div style="background-color: black; margin-top: 143px">.</div> -->

  <script src="/assestsmalefashion/js/jquery-3.3.1.min.js"></script>
  <script src="/assestsmalefashion/js/bootstrap.min.js"></script>
  <script src="/assestsmalefashion/js/jquery.nice-select.min.js"></script>
  <script src="/assestsmalefashion/js/jquery.nicescroll.min.js"></script>
  <script src="/assestsmalefashion/js/jquery.magnific-popup.min.js"></script>
  <script src="/assestsmalefashion/js/jquery.countdown.min.js"></script>
  <script src="/assestsmalefashion/js/jquery.slicknav.js"></script>
  <script src="/assestsmalefashion/js/mixitup.min.js"></script>
  <script src="/assestsmalefashion/js/owl.carousel.min.js"></script>
  <script src="/assestsmalefashion/js/main.js"></script>

  <script src="/assetsuser/js/slick.min.js"></script>
  <script src="/assetsuser/js/jquery.counterup.min.js"></script>
  <script src="/assetsuser/js/jquery.ajaxchimp.min.js"></script>
  <script src="/assetsuser/js/custom.js"></script>

  <script
    type="module"
    src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"
  ></script>
  <script
    nomodule
    src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"
  ></script>
</body>
