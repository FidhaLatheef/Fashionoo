<%- include('userHeader') %>

    <body style="background-color: #fffcfc;">
        <!--::header part start::-->
        <header style="background-color:#fffcfc; width:90vw;margin-left:60px;" class="main_menu home_menu">
            <div class="container">
                <div class="row align-items-center justify-content-center">
                    <div class="col-lg-12">
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand">
                                <a class="navbar-brand" href="index.html"> <img src="img/logo.png" alt="logo"
                                        style="width:35%; height:auto;"> </a>
                            </a>

                            <div class="collapse navbar-collapse main-menu-item" id="navbarSupportedContent">
                                <ul class="navbar-nav">

                                    <li class="nav-item">
                                        <a class="nav-link" href="/about">about</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="/contact">Contact</a>
                                    </li>
                                </ul>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </header>

        <!--================login_part Area =================-->
        <div style="background-color: rgb(255, 254, 254);">
            <section class="login_part section_padding ">
                <div class="container">
                    <div class="row align-items-center">
                        <div class="col-lg-6 col-md-6">
                            <div class="login_part_text text-center">
                                <div class="login_part_text_iner">
                                    <h1></h1>
                                    <h2>Recevied an OTP?</h2>
                                    <p>This is an OTP Email verification for your Signup on
                                        our page and a good example of this is the</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-md-6">
                            <div class="login_part_form">
                                <div class="login_part_form_iner">

                                    <form action="/otp" method="post">
                                        <div class="col-lg-6 col-md-6">
                                            <div class="login_part_form">
                                                <div class="login_part_form_iner">
                                                    <h3>Welcome !! <br>
                                                        Please Enter your OTP</h3>
                                                    <input class="m-2 text-center form-control rounded"
                                                        style="width: 150px;" name="otp" type="text" maxlength="4" />
                                                    <div class="col-md-12 form-group">
                                                        <button type="submit" value="submit" class="btn_3">
                                                            Submit
                                                        </button>
                                                    </div>

                                                    <input type="text" name="email"
                                                        style="height: 0px; visibility: hidden;"
                                                        value="<%= user?.email %>" />
                                                    <input type="text" name="mobile"
                                                        style="height: 0px; visibility: hidden;"
                                                        value="<%= user?.mobile %>" />
                                                    <input type="text" name="password"
                                                        style="height: 0px; visibility: hidden;"
                                                        value="<%= user?.password %>" />
                                                    <input type="text" name="name"
                                                        style="height: 0px; visibility: hidden;"
                                                        value="<%= user?.name %>" />
                                                    <div id="otp"
                                                        class="inputs d-flex flex-row justify-content-center mt-2">

                                                    </div>
                                                </div>
                                    </form>


                                    <% if (typeof ban !=="undefined" ) { %>
                                        <p style="color: red;">
                                            <%= ban %>
                                        </p>
                                        <% } %>
                                            </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
        <!--================login_part end =================-->

        <!--::footer_part start::-->
