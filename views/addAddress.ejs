  <div style="margin-top: 5px; display: flex; justify-content: center">
    <h2
    style="font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif; font-weight: bold; color: rgba(184, 103, 153, 0.767);">
    Add Address</h2>
  </div>

  <div
    style="
      display: flex;
      justify-content: center;
      align-items: center;
      background-color:  rgb(255, 255, 255);
    "
    class="container rounded mt-0 mb-5"
  >
    <div style="width: 50vw; background-color:  rgb(255, 255, 255)">
      <div style="width: 50vw; padding: 30px">
        <form id="addAddressForm" action="/add-address" method="post">
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <label>Name</label>
                <input
                  type="text"
                  name="name"
                  class="form-control p_input"
                  required
                />
              </div>
            </div>

            <div class="col-md-6">
              <div class="form-group">
                <label>Mobile</label>
                <input
                  type="text"
                  name="mobile"
                  class="form-control p_input"
                  maxlength="10"
                  required
                />
              </div>
            </div>

            <div class="col-md-12">
              <div class="form-group">
                <label>Village</label>
                <input
                  type="text"
                  name="village"
                  class="form-control p_input"
                  required
                />
              </div>
            </div>

            <div class="col-md-12">
              <div class="form-group">
                <label>Land Mark</label>
                <input
                  type="text"
                  name="landmark"
                  class="form-control p_input"
                  required
                />
              </div>
            </div>

            <div class="col-md-12">
              <div class="form-group">
                <label>House name</label>
                <input
                  type="text"
                  name="housename"
                  class="form-control p_input"
                  required
                />
              </div>
            </div>

            <div class="col-md-6">
              <div class="form-group">
                <label>City</label>
                <input
                  type="text"
                  name="city"
                  class="form-control p_input"
                  required
                />
              </div>
            </div>

            <div class="col-md-6">
              <div class="form-group">
                <label>Pin code</label>
                <input
                  type="text"
                  name="pincode"
                  class="form-control p_input"
                  required
                />
              </div>
            </div>
          </div>
          <div class="text-center">
            <button type="submit" class="btn_1">
              Add
            </button>
          </div>
        </form>
      </div>
    </div>
  </div>

